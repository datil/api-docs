      &lt;campoAdicional nombre=</span><span class="se">\"</span><span class="s">descripcion</span><span class="se">\"</span><span class="s">&gt;contabilidad@xyz.com&lt;/campoAdicional&gt;
    &lt;/infoAdicional&gt;
